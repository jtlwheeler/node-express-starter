{
  "name": "node-express-starter",
  "version": "1.0.0",
  "description": "Node application with express and Jasmine.",
  "main": "server.js",
  "author": "JT Wheeler",
  "license": "MIT",
  "scripts": {
    "build": "yarn build-ts && yarn tslint",
    "build-serve-watch": "yarn build && concurrently \"yarn watch-serve\" \"yarn watch-ts\"",
    "build-ts": "tsc",
    "clean": "rm -rf dist/",
    "mocha-test": "mocha -r ts-node/register spec/**/myMocha.test.ts",
    "serve": "yarn build && node dist/server.js",
    "start": "yarn serve",
    "test": "jasmine-ts \"./spec/**/*.spec.ts\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "watch-serve": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "chance": "^1.0.13",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "http-status-codes": "^1.3.0",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/chance": "^1.0.0",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/express-jwt": "^0.0.33",
    "@types/jasmine": "^2.8.6",
    "@types/joi": "^13.0.5",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/mongodb": "^3.0.5",
    "@types/mongoose": "^5.0.1",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.0",
    "@types/passport": "^0.4.3",
    "@types/passport-local": "^1.0.32",
    "@types/supertest": "^2.0.4",
    "concurrently": "^3.5.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nodemon": "^1.17.0",
    "supertest": "^3.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
